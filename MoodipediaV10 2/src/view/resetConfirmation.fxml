<AnchorPane id="AnchorPane" prefHeight="188.0" prefWidth="284.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ResetConfirmationController">
   <children>
      <TitledPane fx:id="resetAccountFrame" animated="false" prefHeight="208.0" prefWidth="284.0" text="Reset">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Label layoutX="27.0" layoutY="29.0" prefHeight="52.0" prefWidth="228.0" text="Are you sure you want to reset account?" />
                  <Button fx:id="yesResetButton" layoutX="76.0" layoutY="84.0" mnemonicParsing="false" text="Yes" />
                  <Button fx:id="exitResetButton" layoutX="153.0" layoutY="84.0" mnemonicParsing="false" onAction="#displaySettings" text="Exit" />
                  <Label fx:id="resetAccountMessageLabel" layoutX="73.0" layoutY="133.0" prefHeight="17.0" prefWidth="134.0" />
